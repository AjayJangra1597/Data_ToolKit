    "        # Create a DataFrame to hold the data and determine quadrants\n",
    "        data = pd.DataFrame({'X': x, 'Y': y})\n",
    "        \n",
    "        # Define a function to categorize points into quadrants\n",
    "        def determine_quadrant(row):\n",
    "            if row['X'] >= 0 and row['Y'] >= 0:\n",
    "                return 'Quadrant 1'\n",
    "            elif row['X'] < 0 and row['Y'] >= 0:\n",
    "                return 'Quadrant 2'\n",
    "            elif row['X'] < 0 and row['Y'] < 0:\n",
    "                return 'Quadrant 3'\n",
    "            else:\n",
    "                return 'Quadrant 4'\n",
    "        \n",
    "        # Add a 'Quadrant' column to the DataFrame\n",
